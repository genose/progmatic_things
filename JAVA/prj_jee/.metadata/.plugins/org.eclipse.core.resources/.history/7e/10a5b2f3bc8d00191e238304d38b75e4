package org.genose.java.implementation.javafx.applicationtools;

public abstract interface JFXApplicationValuesAccessor {

	Object pPrivateValue = null;
	
	public Boolean isComplexObject();
	public Object getObjectValue();
	public Boolean setObjectValue(Object aObjectValue );
	
	public Boolean getBoolean();
	public Boolean setBoolean(Boolean aBooleanValue);
	
	public Integer getInteger();
	public Boolean setInteger(Integer aIntegerValue);
	
	public Double getDouble();
	public Boolean setDouble(Double aDoubleValue);
	
	public String getString();
	public Boolean setString(String aStringValue);
	 
	public Object serialize();
	public Object unserialize();
	
	
	
}
