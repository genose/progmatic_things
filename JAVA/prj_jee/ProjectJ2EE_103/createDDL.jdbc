CREATE TABLE Adresse (IDADRESSE INTEGER AUTO_INCREMENT NOT NULL, RUE VARCHAR(255) NOT NULL, TYPEADDR INTEGER NOT NULL, CODEPOSTAL VARCHAR(255), VILLE VARCHAR(255), PRIMARY KEY (IDADRESSE, RUE, TYPEADDR))
CREATE TABLE personne (NOM VARCHAR(255) NOT NULL, PRENOM VARCHAR(255) NOT NULL, AGE INTEGER, ID INTEGER, PRIMARY KEY (NOM, PRENOM))
CREATE TABLE personne_Adresse (NOM VARCHAR(255) NOT NULL, PRENOM VARCHAR(255) NOT NULL, IDADRESSE INTEGER NOT NULL, RUE VARCHAR(255) NOT NULL, TYPEADDR INTEGER NOT NULL, PRIMARY KEY (NOM, PRENOM, IDADRESSE, RUE, TYPEADDR))
ALTER TABLE personne_Adresse ADD CONSTRAINT FK_personne_Adresse_IDADRESSE FOREIGN KEY (IDADRESSE, RUE, TYPEADDR) REFERENCES Adresse (IDADRESSE, RUE, TYPEADDR)
ALTER TABLE personne_Adresse ADD CONSTRAINT FK_personne_Adresse_NOM FOREIGN KEY (NOM, PRENOM) REFERENCES personne (NOM, PRENOM)
